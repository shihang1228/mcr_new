.effect{
  position:relative;       
     -webkit-box-shadow:0 1px 4px rgba(0, 0, 0, 0.3) , 0 0 40px rgba(0, 0, 0, 0.1) inset;
   	   -moz-box-shadow:0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.1) inset; 
    			box-shadow:0 1px 4px rgba(0, 0, 0, 0.3), 0px 0px 40px rgba(0, 0, 0, 0.1) inset;
}
 .effect:before, .effect:after{
    content:"";
	position:absolute; 
	z-index:-1;
	-webkit-box-shadow:0 0 20px rgba(0,0,0,0.8);
	-moz-box-shadow:0 0 20px rgba(0,0,0,0.8);
	box-shadow:0 0 20px rgba(0,0,0,0.8);
	top:50%;
	bottom:0;
	left:10px;
	right:10px;
	-moz-border-radius:100px / 10px;
	border-radius:100px / 10px;
} 

/*现货轨迹*/
.list_title{width:95%;margin:0 auto;border-radius:4px;}
	.list_title li{color:#7b7b7b;}
	.list_title li span{font-weight:bold;}
	.traceD{position:relative;padding:10px;margin-top: 1px;width: 95%;margin: 10px auto;border-radius: 4px;}
	.line{position: absolute;top: 5%;left: 18%;width: 5px;height: 90%;background: #ccc;border-radius: 4px;}
	.traceD li{margin:0px 0px 20px 0px;}
	.traceD li div:nth-child(1){width:18%;}
	.traceD li div:nth-child(2){width:86%;}
	.traceD li .showDate h3{text-align: center;font-weight: bold;font-size:24px;color:rgb(0, 117, 255);}
	.traceD li .showDate h3:nth-child(1){height:21px;}
	.traceD li .showDate h3:nth-child(2){font-size:14px;}
	.traceD li .detail{position: relative;}
	.traceD li .detail .circle{width: 20px;height: 20px;border: 4px solid #f63;border-radius: 10px;position: absolute;top: 20%;left: -7px;background:#fff;}
	.traceD li .detail .woodDetail{width:90%;height:70px;background:#f1c772;margin-left:10%;position:relative;  border-radius: 4px;box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.5);padding:5px 0 0 17px;}
	.traceD li .detail .woodDetail .address{font-size: 16px;font-family: "微软雅黑";font-weight: bold;}
	.traceD li .detail .woodDetail .woodTime{font-size: 15px;}
	.triangle{width:0px;height:0px;overflow:hidden;font-size: 0;line-height: 0;border-color:transparent #f1c772 transparent transparent;border-style:solid;border-width:13px;position: absolute;top:18%;left:-16%;}