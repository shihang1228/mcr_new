label{font-size:17px;font-family:"微软雅黑";color:#000;float:left;margin:0 18px;}
.tzui-diaolog{background:red;color:#666;box-shadow:1px 1px 5px rgba(0,0,0,0.1);position:absolute;left:0px;top:0px;z-index:100}
.tzui-diaolog .tzdialog_title{font-size:15px;line-height:11px;color:#fff;background:#c7000a;padding:10px 15px;margin:0em;outline:none;}
.tzui-diaolog .tzdialog_content{padding:0;margin:0em;background:#fff;border:1px solid #000;border-top:0;}
.tzui-diaolog .tzdialog_message{padding: 40px 15px 0px 15px;text-align:center;}
.tzui-diaolog .tzdialog_panel{text-align:center;padding:10px 0 20px 0;}
.tzui-diaolog .tzdialog_panel input{min-width:100px;text-align:center;}
.tzui-diaolog .tzdialog_ok, .tzui-diaolog .tzdialog_cancel{border:1px solid #0866c6;background:#fff;color:#0866c6;padding:5px 0;cursor:pointer;}
.tzui-diaolog .tzdialog_ok:hover,.tzui-diaolog .tzdialo_cancel:hover{color:#0866c6;border:2px solid #0866c6;padding:5px 0;background:#e5e5e5;}
.drag{width:100px;height:100px;top:0px;left:0px;color:#fff;}
.title{height:50px;background:#114144}
