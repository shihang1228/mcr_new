@charset "UTF-8";
.upImg{position:relative;}
#upload_img{border:1px solid #787878;}
.upImg b{position:absolute;top:0px;left:0px;}
.list li .upImg-display{}
.list li .upImg-display li{float:left;width:19%;margin:1px 2px 3px 0;position:relative;}
