* {
	box-sizing: border-box
}
a:focus {outline-style:none;}
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, fieldset, lengend, button, input, textarea, th, td {
	margin: 0;
	padding: 0
}
h1 {
	font-size: 18px
}
h2 {
	font-size: 16px
}
h3 {
	font-size: 15px;
	font-weight: normal
}
h4, h5, h6 {
	font-size: 100%
}
address, cite, dfn, em, var {
	font-style: normal
}
code, kbd, pre, samp, tt {
	font-family: "Courier New", Courier, monospace
}
small {
	font-size: 12px
}
ul, ol {
	list-style: none
}
a {
	text-decoration: none;
	color: #000
}
a:visited {
	color: #000
}
q:before, q:after {
	content: ''
}
legend {
	color: #000
}
fieldset, img {
	border: none
}
button, input, select, textarea {
	font-size: 100%
}
table {
	border-collapse: collapse;
	border-spacing: 0
}
hr {
	border: none;
	height: 1px
}
@media only screen and (min-width:641px) {
	body {
		padding: 0 35%
	}
}
body {
	background-color: #e8e8e8;
	font: 13px/1 "Segoe UI", "Lucida Grande", Helvetica, Arial, "Microsoft YaHei", FreeSans, Arimo, "Droid Sans", "wenquanyi micro hei", "Hiragino Sans GB", "Hiragino Sans GB W3", "FontAwesome", sans-serif;
	padding-bottom: 35px;
  	padding-top: 46px;
}
.clearfix:after {
	content: ".";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden
}
.ellipsis {
	white-space: nowrap;
	overflow: hidden;
	text-overflow: ellipsis
}
.flex {
	width: 100%;
	display: flex;
	display: -webkit-box
}
.flex>* {
	flex: 1;
	-webkit-box-flex: 1;
	width: 100%
}
.golder>*:first-child {
	width: 38.2%
}
.golder>*:last-child {
	width: 61.8%
}
.golderTurn>*:first-child {
	width: 61.8%
}
.golderTurn>*:last-child {
	width: 38.2%
}
.col1-3>*:first-child {
	width: 25%
}
.col1-3>*:last-child {
	width: 75%
}
.col1-3Turn>*:first-child {
	width: 75%
}
.col1-3Turn>*:last-child {
	width: 25%
}
.hide {
	display: none
}
i {
	font-size: 150%
}
.fullCol {
	width: 100%
}
.header {
	width: 100%;
	height: 35px;
	line-height: 35px;
	background-color: #c7000a;
	margin-bottom: 3px;
	color: #fff;
	text-align: center;
	display: flex;
	display: -webkit-box;
	position: fixed;
	top: 0;
	z-index: 100
}
@media only screen and (min-width:641px) {
	.header {
		width: 30%
	}
}
.header>div {
	flex: 1;
	-webkit-box-flex: 1
}
.header>h1 {
	flex: 6;
	-webkit-box-flex: 6
}
.header a {
	color: #fff;
	display: block
}
.header i {
	line-height: 200%
}
nav.navFixed {
	width: 100%;
	position: fixed;
	bottom: 0;
	background-color: #fff
}
nav.navFixed>a {
	display: block;
	padding: 6px 0;
	color: #000;
	text-align: center
}
.fixedTop {
	position: fixed;
	top: 0
}
input.btnFixed {
	width: 100%;
	height: 50px;
	background-color: #5f9cf9;
	color: #fff;
	font-size: 16px;
	font-weight: bold;
	border: 0;
	border-radius: 0;
	position: fixed;
	bottom: 0
}
@media only screen and (min-width:641px) {
	nav.navFixed {
		width: 30%
	}
}
@media only screen and (min-width:641px) {
	input.btnFixed {
		width: 30%
	}
}
dl.panel {
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.5);
	margin-bottom: 10px
}
dl.panel>dt {
	background-color: #fff;
	padding: 10px 8px;
	color: #000;
	font-weight: bold;
	position: relative
}
dl.panel>dt>i {
	position: absolute;
	right: 12px;
	line-height: 12px
}
dl.panel>dd {
	background-color: #fff;
	padding: 0 8px
}
dl.panel>dd .list {
	padding: 0;
	box-shadow: none;
	margin-bottom: 0
}
dl.panel>dd.borTop {
	border-top: 1px solid #000
}
dl.collapse>dd {
	display: none
}
.list {
	box-shadow: 0 1px 1px rgba(0, 0, 0, 0.5);
	padding: 5px 12px;
	background-color: #fff;
	line-height: 32px;
	margin-bottom: 10px
}
.list>li>a {
	display: flex;
	display: -webkit-box
}
dd.borTop, .list.borTop {
	border-top: 1px solid #000
}
.list.borBottom>li {
	padding: 5px 0;
	border-bottom: 1px dashed #ccc
}
.list.solidBottom>li {
	padding: 5px 0;
	border-bottom: 1px solid #ccc
}
.list.solidBottom>li:last-child {
	border-bottom: 0
}
.user>img {
	width: 50px;
	height: 50px
}
select, button, input {
	-webkit-appearance: none
}
textarea {
	-webkit-appearance: none
}
select, button, input {
	width: 100%;
	height: 28px;
	padding: 5px 8px;
	border: 0;
	border-radius: 4px
}
button {
	width: auto
}
select {
	background: #fff url(img/select.png) no-repeat right;
	background-size: 30px 30px;
	border: 1px solid #000
}
input {
	background: #fff;
	border: 1px solid #000
}
input[type='button'] {
	background: #488AC7;
	color: #fff;
	font-weight: bold
}
.selectWrapper1 {
	width: 95%;
	margin: 5px auto;
}
.selectWrapper{
	width: 95%;
	margin: 10px auto 5px auto;
}
.searchGroup {
	padding: 10px 0 10px 15px;
	margin: 8px;
	border: 1px solid #000;
	border-radius: 4px
}
.searchGroup>input {
	height: 20px;
	padding: 0;
	border: 0
}
.searchGroup>input:first-child {
	width: 60%;
	border-right: 1px solid #000;
	border-radius: 0
}
.searchGroup>input:nth-child(2) {
	width: 38%
}
.onoff input[type='radio'] {
	display: none
}
.onoff label {
	background-color: #DEDEDC;
	display: block;
	text-align: center;
	margin: 2px 0
}
.onoff input[type='radio']:checked+label {
	background-color: #368BC1
}
.checkbox input[type='checkbox'] {
	display: none
}
.checkbox label {
	display: block;
	text-align: center;
	margin: 3px;
	border: 1px dashed #000
}
.checkbox input[type='checkbox']:checked+label {
	background-color: #95B9C7
}
.pos-rel {
  position: relative;;
}