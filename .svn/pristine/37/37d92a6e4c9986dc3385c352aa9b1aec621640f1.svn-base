/*signIn*/
.regLink {
	width: 80%;
	display: block;
	padding: 12px;
	margin: 0 auto;
	background-color: #5f9cf9;
	color: #fff;
	text-align: center;
}
fieldset {
    width: 80%;
    margin: 0 auto;
}
.signIn-Wrapper {
	margin: 50px auto;
}
legend {
    display: none;
}
fieldset>label {
    display: flex;
    display: -webkit-box;
    margin: 20px 0;
    border: 1px solid #000;
}
fieldset input,
fieldset select {
    height: 40px;
}
fieldset>label>i {
    font-size: 20px;
}
fieldset>label>* {
    display: block;
    background-color: #fff;
    border-radius: 0;
    border: 0;
}
fieldset>label>*:nth-child(1) {
    width: 15%;
    background-color: #fff;
    text-align: center;
    line-height: 40px;
}
fieldset>label>*:nth-child(2) {
    width: 75%;
}
fieldset>label>*:nth-child(3) {
    width: 10%;
    font-size: 20px;
    line-height: 40px;
}
fieldset>.smsRight>*:nth-child(2){
	width: 47%;
}
fieldset>.smsRight>*:nth-child(3){
	width: 38.5%;
	height: auto;
	line-height: 28px;
	font-size: 100%;
	text-align: center;
	border: 1px solid #333;
	border-left:1px solid #000;
	box-shadow: 0 1px 2px #8b8b8b inset,0 -1px 0 #3d3d3d inset,0 -2px 3px #8b8b8b inset;
	background: -webkit-linear-gradient(top,#656565,#4c4c4c);
	color: #fff;
	text-shadow: 1px 1px 1px #333;
	overflow: hidden;
}
fieldset>label.text>*:nth-child(1){width: 30%}
fieldset>label.text>*:nth-child(2){width: 70%}
